<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MockHttpServerServlet</servlet-name>
    <servlet-class>com.zjht.hchpserver.MockHttpServerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AsyServletTest</servlet-name>
    <servlet-class>com.zjht.hchpserver.AsyServletTest</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AsynClientStart</servlet-name>
    <servlet-class>com.zjht.asyniobiframework.client.AsynClientStart</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>MockHttpServerServlet</servlet-name>
    <url-pattern>/servlet/MockHttpServerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AsyServletTest</servlet-name>
    <url-pattern>/servlet/AsyServletTest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AsynClientStart</servlet-name>
    <url-pattern>/servlet/clientstart</url-pattern>
  </servlet-mapping>
  
  <listener>  
    <listener-class>com.zjht.hchpserver.AsynSocketListener</listener-class>   
</listener> 	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
